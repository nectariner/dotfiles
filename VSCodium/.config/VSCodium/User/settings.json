{
    "workbench.colorTheme": "Nord",
    "workbench.iconTheme": "material-icon-theme",
    "explorer.confirmDelete": false,
    "window.zoomLevel": 0,
    "editor.fontSize": 18,
    "vim.easymotionMarkerFontFamily": "Inconsolata Regular Nerd Font Complete Mono",
    "vim.easymotionMarkerBackgroundColor": "#7e57c2",
    "vim.easymotionMarkerWidthPerChar": 8,
    "vim.easymotionMarkerFontSize": "20",
    "vim.easymotionMarkerYOffset": 4,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [" "],
            "after": ["leader", "leader", "leader", "b", "d", "w"],
        }
    ],
    "vim.easymotion": true,
    "vim.hlsearch": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "sync.gist": "92eae0ed9f7693c1cd83b06956beaf07",
    "sync.autoDownload": true,
    "sync.autoUpload": true,
    "files.autoSave": "afterDelay",
    "explorer.confirmDragAndDrop": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.fontLigatures": null,
    "editor.fontFamily": "'Inconsolata Nerd Font','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "vscode-neovim.neovimPath": "/usr/bin/nvim",
}
